/* colorUI */
@import "./colorui/main.wxss";
@import "./colorui/icon.wxss";

@import './styles/iconFont.wxss';
/* 在小程序中不支持通配符 * */
page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;
  margin: 0;
  /* 怪异盒子模型 */
  box-sizing: border-box;
}
/* 原生的css和wxss中是支持变量的 */
view{
  /* 主题颜色 */
  --themeColor:#EE2C2C;
}
